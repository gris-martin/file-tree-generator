{
    "name": "file-tree-generator",
    "displayName": "file-tree-generator",
    "description": "generate tree from directory and files",
    "publisher": "Shinotatwu-DS",
    "version": "1.0.2",
    "engines": {
        "vscode": "^1.29.1"
    },
    "icon": "image/filetree_icon.png",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.generateTree"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.generateTree",
                "title": "Generate to Tree"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "explorerResourceIsFolder",
                    "command": "extension.generateTree",
                    "group": "cmdGroup@1"
                }
            ]
        }
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.26",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
